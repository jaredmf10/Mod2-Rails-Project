<h1> WELCOME TO PUPPY LOVE </h1>
<h3> Sign up </h3>

<%= form_for @user do |f| %>
<%= f.label :name, "Name:" %>
<%= f.text_field :name %>

<%= f.label :location, "Location:" %>
<%= f.select :location, Shelter.uniq_locations %> <br>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.submit %>
<% end %>
